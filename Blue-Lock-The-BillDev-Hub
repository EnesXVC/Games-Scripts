    Name = "Auto Steal",
    Description = "Enable auto steal",
    CurrentValue = false,
    Callback = function(Value)
        StealBall = Value
        if Value then
             task.spawn(autoSteal)
        else
            task.cancel(autoSteal)
        end
    end
})
